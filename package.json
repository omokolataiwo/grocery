{
  "name": "basic-react-setup",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/omokolataiwo/basic-react-setup.git",
  "author": "TAIWO KOLAWOLE <kolawole.taiwo@andela.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "postinstall": "npm run build",
    "build": "webpack --config webpack.config.js -p",
    "build:dev": "babel server --out-dir dist --watch",
    "start": "node serve.js",
    "start:dev": "nodemon dist/index.js",
    "webpack:dev": "webpack-dev-server --config webpack.config.js --mode development",
    "all": "npm-run-all --parallel build:dev start:dev lint:watch webpack:dev",
    "lint": "node_modules/.bin/esw web/src",
    "lint:watch": "sane \"node_modules/.bin/esw web/src\" \"web/src\"",
    "test": "jest --updateSnapshot && codecov -t 6ca7b775-fbde-47d2-8f36-f8b0db99f622"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "mongoose": "^5.3.15",
    "notie": "^4.3.1",
    "popper.js": "^1.14.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "validate.js": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react-dev-utils": "^6.0.5",
    "react-hot-loader": "^4.3.11",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "web/src/**/*.{js,jsx}",
      "!web/src/reducers/index.js",
      "!web/src/store/index.js",
      "!web/src/index.jsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -20
      }
    },
    "bail": true,
    "verbose": false,
    "setupFiles": [
      "<rootDir>/web/__tests__/setup/setupEnzyme.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/web/__tests__/setup/",
      "<rootDir>/web/__tests__/__mocks__/"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/web/__tests__/__mocks__/filesMock.js"
    }
  }
}
